@{
    ViewBag.Title = "Cancelacion de Comp.";
}
<div class="pageheader">
    <h1>
        Registrar Pagos del Comprobante
    </h1>
    <div>
        <span class="label" style="color:rgb(144, 157, 163);padding:0px">USTED ESTÁ AQUÍ: </span>
            <ol class="breadcrumb">
                <li>
                    <a href="#">Caja</a>
                </li>
                <li>
                    <a href="#">Cancelación de comprobantes</a>
                </li>
                <li class="active">
                    <a href="#">Bandeja</a>
                </li>
                <li class="active">
                    <a href="#">Registrar pagos</a>
                </li>
            </ol>
    </div>
</div>

<section id="main-content" class="animated fadeInUp">

    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">DATOS DEL COMPROBANTE</h3>
                    <div class="actions pull-right">
                        <i class="fa fa-chevron-down"></i>
                    </div>
                </div>
                <div class="panel-body">
                    <form class="form-horizontal form-border" id="form">
                    <div class="form-group">
                        <div class="col-sm-6">
                        <label class="col-sm-4 control-label">Fecha del comprobante</label>                              
                         <div class = "col-sm-7 input-group date" id="FecCompPagoDt" data-date-format="dd MM yyyy"  data-link-format="yyyy-mm-dd">
                            @Html.TextBoxFor(m => m.CpDisPagoNormal.FecCompPagoDt, String.Format("{0:dd/MM/yyyy}", Model.CpDisPagoNormal.FecCompPagoDt),
                            new
                            {
                                type = "text",
                                propertyname = "CpDisPagoNormal.FecCompPagoDt",
                                @class = "form-control",
                                @readonly="readonly"
                            })
                          </div> 
                        </div>
                      </div>

                    <div class="form-group">
                        <div class="col-sm-6">
                            <label class="col-sm-4 control-label">Tipo de documento</label>  
                            <div class="col-sm-8 row">
                                @Html.TextBoxFor(m => m.CpDisPagoNormal.DescTipoDocumentoVc,
                                new
                                {
                                    maxLength = "10",
                                    propertyname = "CpDisPagoNormal.DescTipoDocumentoVc",
                                    @class = "form-control",
                                    @readonly="readonly"
                                })  
                            </div>
                         </div>
                         <div class="col-sm-6">
                            <label class="col-sm-4 control-label">Nro. comprobante</label>  
                            <div class="col-sm-8 row"> 
                                @Html.TextBoxFor(m => m.CpDisPagoNormal.NroComprobanteVc,
                                new
                                {
                                    msaxLength = "10",
                                    propertyname = "CpDisPagoNormal.NroComprobanteVc",
                                    @class = "form-control",
                                    @readonly="readonly"
                                })  
                            </div>
                        </div>
                    </div>

                      <div class="form-group">
                        <div class="col-sm-6">
                         <label class="col-sm-4 control-label">Nombre del cliente</label>  
                         <div class="col-sm-8 row"> 
                             @Html.TextBoxFor(m => m.CpDisPagoNormal.NombreClienteVc,
                              new
                              {
                                maxLength = "10",
                                propertyname = "CpDisPagoNormal.NombreClienteVc",
                                @class = "form-control",
                                @readonly="readonly" 
                              })
                          </div>
                        </div>
                        <div class="col-sm-6">        
                           <label class="col-sm-4 control-label">Importe total</label>
                            <div class="col-sm-8 row"> 
                                @Html.TextBoxFor(m => m.CpDisPagoNormal.ImporteSolesNu,
                                new
                                {
                                    maxLength = "10",
                                    propertyname = "CpDisPagoNormal.ImporteSolesNu",
                                    @class = "form-control",
                                    @readonly="readonly" 
                                })
                            </div>  
                          </div>
                       </div> 

                    <div class="form-group">
                        <div class="col-sm-6">                            
                            <label class="col-sm-4 control-label">Condición de pago</label>  
                            <div class="col-sm-8 row"> 
                                @Html.TextBoxFor(m => m.CpDisPagoNormal.DescFormaPagoVc,
                                new
                                {
                                      maxLength = "10",
                                      propertyname = "CpDisPagoNormal.DescFormaPagoVc",
                                      @class = "form-control",
                                      @readonly="readonly" 
                                })
                            </div> 
                        </div> 
                        <div class="col-sm-6">   
                            <label class="col-sm-4 control-label">Tipo de cambio</label>  
                            <div class="col-sm-8 row"> 
                                @Html.TextBoxFor(m => m.CpDisPagoNormal.TipoCambioNu,
                                new
                                {
                                    maxLength = "10",
                                    propertyname = "CpDisPagoNormal.TipoCambioNu",
                                    @class = "form-control",
                                    @readonly="readonly" 
                                })
                            </div> 
                        </div>
                    </div>
                 </form>
                </div>
            </div>
        </div>
    </div> 

    <div class="row sBuscaNC">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">DATOS DE LA NOTA DE CRÉDITO</h3>
                    <div class="actions pull-right">
                        <i class="fa fa-chevron-down"></i>
                    </div>
                </div>
                <div class="panel-body">
                    <form class="form-horizontal form-border" id="form">

                        <div class="form-group">
                            <div class="col-sm-6">                            
                                <label class="col-sm-4 control-label">Fecha</label>  
                                <div class="col-sm-6 row">
                                        <input type="hidden" id="IdNC" disabled="disabled"/>
                                        <input type="hidden" id="AnioNC" disabled="disabled"/>
                                        <input type="text" id="FechaNC" class="form-control" disabled="disabled"/>
                                </div> 
                            </div> 
                            <div class="col-sm-6">   
                                <label class="col-sm-4 control-label">Tipo</label>  
                                <div class="col-sm-6 row"> 
                                        <input type="text" id="TipoNC" class="form-control" disabled="disabled"/>
                                </div> 
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-6">                            
                                <label class="col-sm-4 control-label">Documento</label>  
                                <div class="col-sm-6 row"> 
                                        <input type="text" id="DocumentoNC" class="form-control" disabled="disabled"/>
                                </div> 
                            </div> 
                            <div class="col-sm-6">   
                                <label class="col-sm-4 control-label">Anexo</label>  
                                <div class="col-sm-6 row"> 
                                        <input type="text" id="AnexoNC" class="form-control" disabled="disabled"/>
                                </div> 
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-6">                            
                                <label class="col-sm-4 control-label">Importe total</label>  
                                <div class="col-sm-6 row"> 
                                        <input type="text" id="TotalNC" class="form-control" value="0.00" disabled="disabled"/>
                                </div> 
                            </div> 
                            <div class="col-sm-6">   
                                <label class="col-sm-4 control-label">Saldo</label>  
                                <div class="col-sm-6 row"> 
                                        <input type="text" id="SaldoNC" class="form-control" value="0.00" disabled="disabled"/>
                                </div> 
                            </div>
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>

    <div class ="row">
      <div class="col-md-12">
           <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">FORMA DE PAGO</h3>
                </div>
                <div class="panel-body">
                    <form class="form-horizontal form-border" id="form1">
                        <div class="form-group">
                            <div class="col-sm-6">
                                <label class="col-sm-4 control-label">Fecha de pago</label>                              
                                <div class = "col-sm-7 input-group date" id="FecPagoDt" data-date-format="dd MM yyyy"  data-link-format="yyyy-mm-dd">
                                @Html.TextBoxFor(m => m.CpDisPagoNormal.FecPagoDt, String.Format("{0:dd/MM/yyyy}", Model.CpDisPagoNormal.FecPagoDt),
                                new
                                {
                                    type = "text",
                                    propertyname = "CpDisPagoNormal.FecPagoDt",
                                    @class = "form-control",
                                    @readonly="readonly"
                                })
                                <div class="input-group-addon"> 
                                    <span class="glyphicon glyphicon-calendar"
                                    </span> 
                                    </div> 
                                </div> 
                            </div>
                            <div class="col-sm-6 BuscaNC">
                                <label class="col-sm-4 control-label">Nota de crédito</label>  
                                <div class="col-sm-7 row">
                                    <button class="btn btn-info" onclick="$('#Criterio').val(''); $('#ListaNotasCredito').html(''); $('#ModalNotaCredito').modal('show'); return false;"><i class="fa fa-search"></i> Buscar</button>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-6">
                                <label class="col-sm-4 control-label">Forma de pago</label>  
                                <div class="col-sm-7 row">
                                    @Html.DropDownListFor(m => m.CpDisPagoNormal.IdFormaPagoNu, Model.CmbFormaDePago,
                                    new 
                                    { 
                                        propertyname = "CpDisPagoNormal.IdFormaPagoNu", 
                                        @class = "form-control",
                                        @onchange="SelFormaPago()"
                                    })
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <label class="col-sm-4 control-label">Tipo de moneda</label>  
                                <div class="col-sm-7 row">
                                    @Html.DropDownListFor(m => m.CpDisPagoNormal.IdTipoMonedaNu, Model.CmbTipoMoneda,
                                    new 
                                    { 
                                        propertyname = "CpDisPagoNormal.IdTipoMonedaNu", 
                                        @class = "form-control",
                                        disabled = "disabled"
                                    })
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-6">
                                <label class="col-sm-4 control-label">Banco</label>  
                                <div class="col-sm-7 row">
                                    @Html.DropDownListFor(m => m.CpDisPagoNormal.IdBancoNu, Model.CmbBancos,
                                    new 
                                    { 
                                        propertyname = "CpDisPagoNormal.IdBancoNu", 
                                        @class = "form-control",
                                        disabled = "disabled"
                                    })
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <label class="col-sm-4 control-label">Nro. de operación</label>
                                <div class="col-sm-7 row">
                                    @Html.TextBoxFor(m => m.CpDisPagoNormal.NroOperacionVc,
                                    new
                                    {
                                        maxLength = "10",
                                        propertyname = "CpDisPagoNormal.NroOperacionVc",
                                        @class = "form-control",
                                        disabled = "disabled"
                                    })
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-6">
                                <label class="col-sm-4 control-label">Monto</label>  
                                <div class="col-sm-7 row">
                                    @Html.TextBoxFor(m => m.CpDisPagoNormal.ImporteNu,
                                    new
                                    {
                                        maxLength = "10",
                                        propertyname = "CpDisPagoNormal.ImporteNu",
                                        @class = "form-control",
                                        @onkeypress = "return isNumberKey(event, this)"
                                           
                                    })
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <label class="col-sm-4 control-label"></label>  
                                <div class="col-sm-7 row">
                                    <button id="AgregarPagoBtn" type="button" class="btn btn-info"
                                        onclick="AgregarPago();"> 
                                        <i class="fa fa-plus"></i> AGREGAR PAGO
                                    </button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>     
    </div>

    <div id="div_GrillaDetallePagos">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">detalle de pagos</h3>
            </div>
            <div class="panel-body table-responsive">
                <div class="col-sm-12  table-responsive">
                    <table id="FormaPagoTable" class="table table-striped table-bordered table-hover table-condensed " cellspacing="0">
                        <thead>
                            <tr>
                                <th class="text-center"><p>FORMA PAGO</p></th>
                                <th class="text-center"><p>BANCO</p></th> 
                                <th class="text-center"><p>Nº OPERACIÓN</p></th>
                                <th class="text-center"><p>DÓLARES</p></th>
                                <th class="text-center"><p>SOLES</p></th>
                                <th class="text-center"><p>ACCIÓN</p></th>
                            </tr>
                        </thead>
                        <tbody id="DetallePagos">
                        </tbody>
                    </table>
                    <hr />
                    <form>
                        <div class="col-sm-4" align="left">
                            <label class="col-sm-5 control-label"><h3 style="text-align:right;margin-top:8px;"><b>DIFERENCIA S/.</b></h3></label>  
                            <div class="col-sm-6">
                                <input id="ImporteDiferencia" type="text" class="form-control" value="0.00" disabled="disabled"/>
                            </div>
                            </div>
                        <div class="col-sm-4" align ="left">
                            <label class="col-sm-5 control-label"><h3 style="text-align:right;margin-top:8px;"><b>TOTAL $.</b></h3></label>  
                            <div class="col-sm-6">
                                <input id="ImporteTotalDolares" type="text" class="form-control" value="0.00" disabled="disabled"/>
                            </div>
                            </div>   
                        <div class="col-sm-4" align ="left">
                            <label class="col-sm-5 control-label"><h3 style="text-align:right;margin-top:8px;"><b>TOTAL S/.</b></h3></label>  
                            <div class="col-sm-6">
                                <input id="ImporteTotalSoles" type="text" class="form-control" value="0.00" disabled="disabled"/>
                            </div>
                        </div>   
                    </form>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-body text-center">
                    <button id="guardarBtn" type="button" class="btn btn-success" 
                        onclick="Guardar();">
                        <i class="fa  fa-save"></i> Guardar
                    </button>                    
                    <button id="agregarBtn" type="button" class="btn btn-default"
                        onclick="Cancelar();">
                        <i class="fa fa-reply"></i> Cancelar
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<div class="modal fade bs-example-modal-lg" id="ModalNotaCredito" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="overflow:scroll">
