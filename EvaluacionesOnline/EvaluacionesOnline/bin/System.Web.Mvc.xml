        private IList<TrasladoBE> MapDataReaderToList(IDataReader oR)
        {
            var lista = new List<TrasladoBE>();

            while (oR.Read())
            {
                lista.Add(MapDataReaderToEntity(oR));
            }

            return lista;
        }

        private TrasladoBE MapDataReaderToEntity(IDataReader oR)
        {
            var item = new TrasladoBE();

            if (map == 7)
            {
                item.DescOrigenVc           = oR["SUCURSAL_ORIGEN"].ToText();
                item.DescDestinoVc          = oR["SUCURSAL_DESTINO"].ToText();
                item.NroTrasladoVc          = oR["NUMERO_TRASLADO"].ToText();
                item.FecTrasladoDt          = oR["FECHA_TRASLADO"].ToDateTime();
                item.CodProveedorVc         = oR["COD_PROVEEDOR"].ToText();
                item.CodProductoVc          = oR["CODIGO_PRODUCTO"].ToText();
                item.DescProductoVc         = oR["NOMBRE_PRODUCTO"].ToText();
                item.CantidadNu             = oR["NU_CANTIDAD"].ToInt32();
                item.NroSerieVc             = oR["IMEI"].ToText();
                item.NroSerieValor1Vc       = oR["ICC"].ToText();
                item.GuiaRemisionVc         = oR["GUIA_REMISION"].ToText();
                item.FecRecepcionDt         = oR["FECHA_RECEPCION"].ToDateTime();
                return item;
            }

            if (map == 4)
            {
                item.NroSerieVc = oR["VC_NRO_SERIE"].ToText();
                item.NroGuiaVc = oR["NU_NRO_DOCUMENTO"].ToText();
                return item;
            }
            if (map == 5)
            {
                item.NroSerieVc = oR["VC_NRO_SERIE"].ToText();
                item.NroGuiaVc = oR["NU_NRO_DOCUMENTO"].ToText();
                item.IdOpcionNu = int.Parse(oR["NU_UBICACION"].ToText());
                return item;
            }

            if (map == 6)
            {
                item.NroSerieVc = oR["SERIE"].ToText();
                item.NroGuiaVc = oR["NRO_GUIA"].ToText();
                item.FecGuiaDt = oR["DT_FEC_GUIA"].ToDateTime();
                item.FecTrasladoDt = oR["DT_FEC_TRASLADO"].ToDateTime();
                item.DescDestinoVc = oR["DIRECCION_DEST"].ToText();
                item.DiaVc = oR["DIA"].ToText();
                item.MesVc = oR["MES"].ToText();
                item.AnioVc = oR["AO"].ToText();
                item.DescDestinoVc = oR["DESTINO"].ToText();
                item.DescOrigenVc = oR["ORIGEN"].ToText();
                item.DireccionOrigenVc = oR["DIRECCION_ORIGEN"].ToText();
                item.DireccionDestinoVc = oR["DIRECCION_DESTINO"].ToText();
                item.ConductorVc = oR["VC_CONDUCTOR"].ToText();
                item.DocIdentConductorVc = oR["VC_DOC_IDENT_CONDUCTOR"].ToText();
                item.LicenciaConductoVc = oR["VC_LICENCIA_CONDUCTO"].ToText();
                item.PlacaTransporteVc = oR["VC_PLACA_TRANSPORTE"].ToText();
                item.CodProductoVc = oR["VC_COD_PRODUCTO"].ToText();
                item.DescProductoVc = oR["VC_DESC_PRODUCTO"].ToText();
                item.DesProdLarga = oR["VC_DESC_PROD_LARGA"].ToText();
                item.CantidadNu = oR["CANTIDAD"].ToInt();
                item.ImpTotalNu = oR["IMP_TOTAL"].ToDecimal();
                item.DescUnidadVc = oR["VC_DESC_UNIDAD_MEDIDA"].ToText();
                item.DescTransporteVc = oR["DESC_TRANSPORTE"].ToText();
                item.CertInscripcionVc = oR["CERT_INSCRIPCION"].ToText();
                item.SerieVc = oR["SERIES"].ToText();
                return item;
            }
            
            item.IdEmpresaNu= oR["NU_ID_EMPRESA"].ToInt32();
            item.IdSucursalNu= oR["NU_ID_SUCURSAL"].ToInt32();
            item.AnioTrasladoCh= oR["CH_ANIO_TRASLADO"].ToText();
            item.IdTrasladoNu= oR["NU_ID_TRASLADO"].ToInt32();
            item.FecTrasladoDt= oR["DT_FEC_TRASLADO"].ToDateTime();
            item.NroSerieVc= oR["VC_NRO_SERIE"].ToText();
            item.NroGuiaVc= oR["VC_NRO_GUIA"].ToText();
            item.FecGuiaDt= oR["DT_FEC_GUIA"].ToDateTime();
            item.ConductorVc= oR["VC_CONDUCTOR"].ToText();
            item.DocIdentConductorVc = oR["VC_DOC_IDENT_CONDUCTOR"].ToText();
            item.LicenciaConductoVc= oR["VC_LICENCIA_CONDUCTO"].ToText();
            item.PlacaTransporteVc= oR["VC_PLACA_TRANSPORTE"].ToText();
            item.IdEmpresaOrigNu= oR["NU_ID_EMPRESA_ORIG"].ToInt32();
            item.IdSucursalOrigNu= oR["NU_ID_SUCURSAL_ORIG"].ToInt32();
            item.IdAlmacenOrigNu= oR["NU_ID_ALMACEN_ORIG"].ToInt32();
            item.IdEmpresaDestNu= oR["NU_ID_EMPRESA_DEST"].ToInt32();
            item.IdSucursalDestNu= oR["NU_ID_SUCURSAL_DEST"].ToInt32();
            item.IdAlmacenDestNu= oR["NU_ID_ALMACEN_DEST"].ToInt32();
            item.ImporteTotalNu= oR["NU_IMPORTE_TOTAL"].ToInt32();
            item.ObservacionVc= oR["VC_OBSERVACION"].ToText();
            item.ValidadoCh= oR["CH_VALIDADO"].ToText();
            item.FecValidacionDt= oR["DT_FEC_VALIDACION"].ToDateTime();
            item.UsrValidacionVc= oR["VC_USR_VALIDACION"].ToText();
            item.RecepcionadoCh= oR["CH_RECEPCIONADO"].ToText();
            item.FecRecepcionDt= oR["DT_FEC_RECEPCION"].ToDateTime();
            item.UsrRecepcionVc= oR["VC_USR_RECEPCION"].ToText();
            item.EstadoCh= oR["CH_ESTADO"].ToText();
            item.FecRegDt= oR["DT_FEC_REG"].ToDateTime();
            item.UsrRegVc= oR["VC_USR_REG"].ToText();
            item.FecModDt= oR["DT_FEC_MOD"].ToDateTime();
            item.UsrModVc= oR["VC_USR_MOD"].ToText();
            item.StatusCh= oR["CH_STATUS"].ToText();
            if (map == 1)
            {
                item.NroTrasladoVc = oR["VC_NRO_TRASLADO"].ToText();
                item.DescOrigenVc = oR["ORIGEN"].ToText();
                item.DescDestinoVc = oR["DESTINO"].ToText();
