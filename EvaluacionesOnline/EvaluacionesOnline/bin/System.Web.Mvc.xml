@section Styles {
    @Styles.Render("~/plugins/dataTablesCSS")
}

@section Scripts {
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/plugins/dataTables-bootstrap")
    @Scripts.Render("~/plugins/validate")  
    @Scripts.Render("~/plugins/maskedinput")

    <script type="text/javascript" src="@Url.Content("~/Scripts/UpdateField.js")"></script>   
    <script type="text/javascript" src="@Url.Content("~/Scripts/BindingInput.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/BindingOutput.js")"></script>

    <script type="text/javascript" src="~/Scripts/plugins/datetimepicker/bootstrap-datetimepicker.js" charset="UTF-8"></script>
    <script type="text/javascript" src="~/Scripts/plugins/datetimepicker/locales/bootstrap-datetimepicker.es.js" charset="UTF-8"></script>
    <link href="~/Content/bootstrap-datetimepicker.min.css" rel="stylesheet" />

    <script>
        var MaxRedondeo = "@Model.ImporteRedondeoNu";

        var id = "@Model.CpDisPagoNormal.IdCompPagoNu";
        var anio = "@Model.CpDisPagoNormal.AnioCompPagoCh";
        var formaPago = "@Model.CpDisPagoNormal.IdTipoVentaNu";
        var idCliente = "@Model.CpDisPagoNormal.IdClienteNu"

        $(document).ready(function () {
            datetimepicker('#FecPagoDt');
            $('#CpDisPagoNormal_ImporteNu').val('0.00');
            Calc();
            $('#CpDisPagoNormal_ImporteSolesNu').val(parseFloat($('#CpDisPagoNormal_ImporteSolesNu').val()).toFixed(2));
            $('#Criterio').keypress(enterBuscar);

            if (formaPago == 13) {
                $('#CpDisPagoNormal_IdFormaPagoNu').attr('disabled', 'disabled');
                $('#CpDisPagoNormal_IdFormaPagoNu').val(5);
                $('#CpDisPagoNormal_IdBancoNu').attr('disabled', 'disabled');
                $('#CpDisPagoNormal_IdTipoMonedaNu').attr('disabled', 'disabled');
                $('#CpDisPagoNormal_NroOperacionVc').attr('disabled', 'disabled');
            }
            else {
                $('.BuscaNC').attr('style', 'visibility:hidden');
            }

        });

        function enterBuscar(evt) {
            if (evt.which == 13) {
                Buscar();
                return false;
            }
            return true;
        }

        function Buscar() {
            var model = new Object();
            var NotaCredito = { IdClienteNu: idCliente, CriterioVc: $('#Criterio').val() }

            model.NotaCredito = NotaCredito;
            $("#SaveMensaje").html('<i class="fa  fa-refresh" style="color:green"></i> Buscando notas de crédito, espere un momento...');
            $("#ModalSave").modal('show');
            $.ajax({
                type: "POST",
                url: window.GetControllerUrl("BuscarNotaCredito", "PagoComprobantesNormal"),
                data: model,
                success: function (result) {
                    $('#ListaNotasCredito').html(result);
                    $("#ModalSave").modal('hide');
                    $("#ModalComprobante").modal('show');
                },
                error: function (result) {
                    alert("Ha ocurrido un error inesperado contactar con el área de sistemas.")
                }
            });
        }

        $(document).on('click', '.Selnotacredito', function () {
            var item = $(this).attr('name').split('~');
            $('#IdNC').val(item[0]);
            $('#AnioNC').val(item[1]);
            $('#FechaNC').val(item[2]);
            var tipo = "";
            switch (item[3]) {
                case 'T': tipo = "TOTAL"; break;
                case 'P': tipo = "PARCIAL"; break;
                case 'D': tipo = "DESCUENTO"; break;
            }

            $('#TipoNC').val(tipo);
            $('#DocumentoNC').val(item[5]);
            $('#CpDisPagoNormal_NroOperacionVc').val(item[5]);
            $('#AnexoNC').val(item[4]);
            $('#TotalNC').val(parseFloat(item[6]).toFixed(2));

            var saldoReal = parseFloat(item[7]).toFixed(2);

            $('#DetallePagos tr').each(function () {
                if ($('#DocumentoNC').val() == $(this).find('td').eq(2).text().trim()) {
                    saldoReal = saldoReal - parseFloat($(this).find('td').eq(4).text().trim());
                }
            });

            for (var i = 0; i < notasDetalle.length; i++) {
                if ($('#DocumentoNC').val() == notasDetalle[i].split('~')[0]) {
                    saldoReal = saldoReal + parseFloat(notasDetalle[i].split('~')[1]);
                }
            }

            $('#SaldoNC').val(parseFloat(saldoReal).toFixed(2));
            $('#CpDisPagoNormal_ImporteNu').val('0.00');
            $('#ModalNotaCredito').modal('hide');
        })

        function LimpiarNC() {
            $('#IdNC').val('');
            $('#AnioNC').val('');
            $('#FechaNC').val('');
            $('#TipoNC').val('');
            $('#DocumentoNC').val('');
            $('#AnexoNC').val('');
            $('#TotalNC').val('0.00');
            $('#SaldoNC').val('0.00');
        }

        function datetimepicker(identifier) {
            $(identifier).datetimepicker({
                language: 'es',
                weekStart: 1,
                todayBtn: 1,
                autoclose: 1,
                todayHighlight: 1,
                startView: 2,
                minView: 2,
                forceParse: 0,
                format: 'dd/mm/yyyy'
            });
        }

        function Guardar()
        {
            if ($('#ImporteDiferencia').val() != 0) {
                $("#SaveMensaje").html('<i class="fa fa-remove" style="color:darkred"></i> Debe pagar el total del importe facturado por renta adelantada.');
                $('#ModalSave').modal('show');
                return false;
            }
            $("#SaveMensaje").html('<i class="fa fa-refresh" style="color:darkgreen"></i> Se está registrando la cancelación de rentas adelantadas, espere un momento...');
            $('#ModalSave').modal('show');

            var model = new Object();

            var ListaPagoNormal = [];
            $('#DetallePagos tr').each(function () {
                ListaPagoNormal[ListaPagoNormal.length] = {
                    AnioCompPagoCh      : anio,
                    IdCompPagoNu        : id,
                    IdTipoVentaNu       : formaPago,
                    IdClienteNu         : idCliente,
                    IdTipoMonedaNu      : $(this).attr('name').split('~')[2],
                    IdFormaPagoNu       : $(this).attr('name').split('~')[1],
                    NroOperacionVc      : $(this).attr('name').split('~')[4],
                    FecPagoDt           : $('#CpDisPagoNormal_FecPagoDt').val(),
                    ImporteNu           : (parseFloat($(this).attr('name').split('~')[5])/parseFloat($(this).attr('name').split('~')[6])),
                    TipoCambioNu        : $(this).attr('name').split('~')[6],
                    ImporteSolesNu      : $(this).attr('name').split('~')[5],
                    IdBancoNu           : $(this).attr('name').split('~')[3],
                    IdNotaCreditoNu     : $(this).attr('name').split('~')[7],
                    AnioNotaCreditoCh   : $(this).attr('name').split('~')[8],
                    IdOpcionNu          : 0
                }
            })

            var CpDisPagoNormal = { ListaPagoNormal: ListaPagoNormal }

            model.CpDisPagoNormal = CpDisPagoNormal;

            $.ajax({
                type: "POST",
                url: GetControllerUrl("Guardar", "PagoComprobantesNormal"),
                data: model,
                success: function (resultado)
                {
                    if (resultado.message.split('~')[0] != "0")
                    {
                        $("#SaveMensaje").html('<i class="fa fa-check" style="color:darkgreen"></i>' + resultado.message.split('~')[1]);
                        $(document).on('click', function () {
                            window.location.href = GetControllerUrl("Index", "PagoComprobantesNormal");
                        })
                    }
                    else
                    {
                        $("#SaveMensaje").html('<i class="fa fa-remove" style="color:darkred"></i> ' + resultado.message.split('~')[1]);
                    }
                },
                error: function (result) {
                    alert("Ha sucedido un error inesperado contactar con el área de sistemas.")
                }
          });
        }

        function ValidarSaldo() {
            if ($('#CpDisPagoNormal_IdFormaPagoNu').val() == 5 && $('#CpDisPagoNormal_ImporteNu').val() != "" && $('#CpDisPagoNormal_ImporteNu').val() != "0.00" && ("0000000000" + parseFloat($('#CpDisPagoNormal_ImporteNu').val()).toFixed(2)).slice(-13) > ("0000000000" + parseFloat($('#SaldoNC').val()).toFixed(2)).slice(-13)) {
                $('#CpDisPagoNormal_ImporteNu').val($('#SaldoNC').val());
                $('#SaveMensaje').html('<i class="fa fa-remove" style="color:darkred;"></i> El monto a pagar no debe ser mayor al saldo de la nota de crédito');
                $('#ModalSave').modal('show');
            }
            else {
                $('#CpDisPagoNormal_ImporteNu').val(parseFloat($('#CpDisPagoNormal_ImporteNu').val()).toFixed(2));
            }
        }

        var btnAuto = 0
        function AgregarPago() {

            var IdNotaCreditoNu     = null;
            var AnioNotaCreditoNu   = null;
            if ($('#CpDisPagoNormal_IdFormaPagoNu').val() == 5) {
                IdNotaCreditoNu = $('#IdNC').val();
                AnioNotaCreditoNu = $('#AnioNC').val();

                if (IdNotaCreditoNu == "" && AnioNotaCreditoNu == "") {
                    $('#SaveMensaje').html('<i class="fa fa-remove" style="color:darkred;"></i> Debe buscar y seleccionar la nota de crédito con la que realizará el pago');
                    $('#ModalSave').modal('show');
                    return;
                }
            }

            if ($('#CpDisPagoNormal_IdFormaPagoNu').val() == '')
            {
                $("#SaveMensaje").html('<i class="fa fa-remove" style="color:darkred"></i> Debe definir la forma de pago.');
                $('#ModalSave').modal('show');
                return false;
            }

            if (('0000000000' + $('#CpDisPagoNormal_ImporteNu').val()).trim().slice(-13) == ('0000000000.00').trim()) {
                $("#SaveMensaje").html('<i class="fa fa-remove" style="color:darkred"></i> Debe ingresar el monto a pagar.');
                $('#ModalSave').modal('show');
                return false;
            }

            if (parseFloat($('#CpDisPagoNormal_ImporteNu').val()) > parseFloat($('#ImporteDiferencia').val())) {
                $("#SaveMensaje").html('<i class="fa fa-remove" style="color:darkred"></i> El monto no puede ser mayor a la diferencia.');
                $('#ModalSave').modal('show');
                return false;
            }

            if ($('#CpDisPagoNormal_IdFormaPagoNu').val() == 26 || $('#CpDisPagoNormal_IdFormaPagoNu').val() == 27 || $('#CpDisPagoNormal_IdFormaPagoNu').val() == 38) {
                if ($('#CpDisPagoNormal_IdBancoNu').val() == '' || $('#CpDisPagoNormal_NroOperacionVc').val() == '') {
                    $("#SaveMensaje").html('<i class="fa fa-remove" style="color:darkred"></i> Debe definir un banco e ingresar el número de operación.');
                    $('#ModalSave').modal('show');
                    return false;
                }
            }


            if ($('#CpDisPagoNormal_IdFormaPagoNu').val() == 6) {
                if (parseFloat($('#CpDisPagoNormal_ImporteNu').val()) > parseFloat(MaxRedondeo) || parseFloat($('#CpDisPagoNormal_ImporteNu').val()) < parseFloat((MaxRedondeo * -1))) {
                    $('#CpDisPagoNormal_ImporteNu').focus();
                    $('#SaveMensaje').html('<i class="fa fa-remove" style="color:darkred;"></i> El importe por redondeo no puede ser mayor a <strong>' + parseFloat(MaxRedondeo).toFixed(2) + '</strong> ni menor a <strong>-' + parseFloat(MaxRedondeo).toFixed(2) + '</strong>');
                    $('#ModalSave').modal('show');
                    return false;
                }
            }
            else {
                if ($('#CpDisPagoNormal_ImporteNu').val().toString().indexOf('-') > -1) {
                    $('#CpDistribucionPago_ImporteSolesNu').focus();
                    $('#SaveMensaje').html('<i class="fa fa-remove" style="color:darkred;"></i> No puede ingresar un importe negativo para la forma de pago seleccionada.');
                    $('#ModalSave').modal('show');
                    return false;
                }
            }

            var Banco = '';
            if ($('#CpDisPagoNormal_IdBancoNu').val() != '')
            {
                Banco = $('#CpDisPagoNormal_IdBancoNu option:selected').text();
            }

            var data =  $('#CpDisPagoNormal_FecPagoDt').val()         + "~" +
                        $('#CpDisPagoNormal_IdFormaPagoNu').val()     + "~" +
                        $('#CpDisPagoNormal_IdTipoMonedaNu').val()    + "~" +
                        $('#CpDisPagoNormal_IdBancoNu').val()         + "~" +
                        $('#CpDisPagoNormal_NroOperacionVc').val()    + "~" +
                        $('#CpDisPagoNormal_ImporteNu').val()         + "~" +
                        $('#CpDisPagoNormal_TipoCambioNu').val()      + "~" +
                        IdNotaCreditoNu                               + "~" +
                        AnioNotaCreditoNu;

            var Html = '<tr id="Pago' + btnAuto + '" name="' + data + '">' +
                            '<td class="text-center">' + $('#CpDisPagoNormal_IdFormaPagoNu option:selected').text() + '</td>' +
                            '<td class="text-center">' + Banco + '</td>' +
                            '<td class="text-center">' + $('#CpDisPagoNormal_NroOperacionVc').val() + '</td>' +
                            '<td class="text-center">' + (parseFloat($('#CpDisPagoNormal_ImporteNu').val()) / parseFloat($('#CpDisPagoNormal_TipoCambioNu').val())).toFixed(6) + '</td>' +
                            '<td class="text-center">' + $('#CpDisPagoNormal_ImporteNu').val() + '</td>' +
                            '<td class="text-center">'+
                                '<button value="' + btnAuto + '" class="btn btn-danger" onClick="DeleteItem(' + btnAuto + ');"><i class="fa fa-remove"></i></button>'
                            '</td>'+
                        '</tr>';
            btnAuto++;            
            $('#DetallePagos').append(Html);
            Calc();
            if (formaPago == 3) {
                $('#CpDisPagoNormal_IdFormaPagoNu').val('');
            }
            else {
                $('#CpDisPagoNormal_IdFormaPagoNu').val('5');
            }
            SelFormaPago();
            LimpiarNC();
        }

        var itemPago = 0;
        function DeleteItem(IdCpDistPagoNu) {
            $('#ConfirmMensaje').html("¿Desea quitar el pago seleccionado?");
            $('#ModalConfirm').modal('show');
            itemPago = IdCpDistPagoNu;
        }

        function DeleteItemConfirmado() {
            $('#Pago' + itemPago).remove();
            $('#ModalConfirm').modal('hide');
            LimpiarNC();
            Calc();
        }

        function SelFormaPago() {
            if ($('#CpDisPagoNormal_IdFormaPagoNu').val() == 26 || $('#CpDisPagoNormal_IdFormaPagoNu').val() == 27 || $('#CpDisPagoNormal_IdFormaPagoNu').val() == 38) {
                $('#CpDisPagoNormal_IdBancoNu').removeAttr('disabled');
                $('#CpDisPagoNormal_NroOperacionVc').removeAttr('disabled');
            }
            else {
                $('#CpDisPagoNormal_IdBancoNu').attr('disabled', 'disabled');
                $('#CpDisPagoNormal_NroOperacionVc').attr('disabled', 'disabled');
                $('#CpDisPagoNormal_IdBancoNu').val('');
                $('#CpDisPagoNormal_NroOperacionVc').val('');                
            }
            if ($('#CpDisPagoNormal_IdFormaPagoNu').val() == 5) {
                $('.BuscaNC').removeAttr('style');
            }
            else {
                $('.BuscaNC').attr('style','visibility:hidden');
            }

            $('#CpDisPagoNormal_ImporteNu').val('0.00');
        }

        $(document).on('blur', '#CpDisPagoNormal_ImporteNu', function () {
            if ($('#CpDisPagoNormal_ImporteNu').val() == "") { $('#CpDisPagoNormal_ImporteNu').val('0.00'); }
            else { $('#CpDisPagoNormal_ImporteNu').val(parseFloat($('#CpDisPagoNormal_ImporteNu').val()).toFixed(2)); }
            ValidarSaldo();
        })

        function Calc() {
            var pTotal = 0;
            $('#DetallePagos tr').each(function(){                
                pTotal += parseFloat($(this).attr('name').split('~')[5]);
                $(this).find('td').eq(3).text(parseFloat($(this).find('td').eq(3).text().trim()).toFixed(6))
                $(this).find('td').eq(4).text(parseFloat($(this).find('td').eq(4).text().trim()).toFixed(2))
            })
            $('#ImporteTotalSoles').val(pTotal.toFixed(2));
            $('#ImporteTotalDolares').val((parseFloat($('#ImporteTotalSoles').val()) / parseFloat($('#CpDisPagoNormal_TipoCambioNu').val())).toFixed(6));
            $('#ImporteDiferencia').val( (parseFloat($('#CpDisPagoNormal_ImporteSolesNu').val()) - parseFloat($('#ImporteTotalSoles').val())).toFixed(2) );
        }

        
        function Cancelar() {
            window.location.href = GetControllerUrl("Index", "PagoComprobantesNormal");
        }

        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode;
            if (charCode != 13) {
                if (charCode != 45 && charCode != 46 && charCode > 31
                  && (charCode < 48 || charCode > 57))
                    return false;
                return true;
            }
            else {
                ValidarSaldo();
                return false;
            }
        }      

    </script>
