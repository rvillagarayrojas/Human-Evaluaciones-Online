namespace Siscom.Data.Almacen
{
    public class TrasladoDA : BaseAbstractDA
    {
        public TrasladoDA()
            : base()
        {
        }
        int map = 0;
        public IList<TrasladoBE> List(TrasladoBE oItem)
        {   
            try
            {
                var cmd = db.GetStoredProcCommand("SP_SEL_TRASLADO");
                if (oItem.IdOpcionNu == 4)
                {
                    cmd = db.GetStoredProcCommand("SP_GET_SERIES_IMPRESION");

                    db.AddInParameter(cmd, "@K_NU_ID_EMPRESA", DbType.Decimal, oItem.IdEmpresaNu);
                    db.AddInParameter(cmd, "@K_NU_ID_SUCURSAL", DbType.Decimal, oItem.IdSucursalNu);
                    db.AddInParameter(cmd, "@K_NU_ID_TIPO_DOCUMENTO", DbType.Decimal, oItem.IdTipoDocumentoNu);
                    db.AddInParameter(cmd, "@K_CH_TIPO_FORMATO_IMPRESION", DbType.String, oItem.TipoFormatoImpresionCh);
                    db.AddInParameter(cmd, "@K_NU_ID_DIA_SEMANA", DbType.Decimal, oItem.IdDiaSemanaNu);
                    db.AddInParameter(cmd, "@K_NU_ID_EMPLEADO", DbType.Decimal, oItem.IdEmpleadoNu);
                    map = 4;
                }
                else if (oItem.IdOpcionNu == 5)
                {
                    cmd = db.GetStoredProcCommand("SP_GET_TRASLADO_SERIE_GUIA");

                    db.AddInParameter(cmd, "@NU_ID_EMPRESA", DbType.Decimal, oItem.IdEmpresaNu);
                    db.AddInParameter(cmd, "@NU_ID_SUCURSAL", DbType.Decimal, oItem.IdSucursalNu);
                    db.AddInParameter(cmd, "@CH_ANIO_TRASLADO", DbType.String, oItem.AnioTrasladoCh);
                    db.AddInParameter(cmd, "@NU_ID_TRASLADO", DbType.Decimal, oItem.IdTrasladoNu);
                    db.AddInParameter(cmd, "@VC_NRO_SERIE", DbType.String, oItem.NroSerieVc);
                    db.AddInParameter(cmd, "@VC_NRO_GUIA", DbType.String, oItem.NroGuiaVc);
                    map = 5;
                }
                else if (oItem.IdOpcionNu == 6)
                {
                    cmd = db.GetStoredProcCommand("SP_GET_REPORTE_TRASLADO");
                    db.AddInParameter(cmd, "@K_NU_ID_EMPRESA", DbType.Decimal, oItem.IdEmpresaNu);
                    db.AddInParameter(cmd, "@K_NU_ID_SUCURSAL", DbType.Decimal, oItem.IdSucursalNu);
                    db.AddInParameter(cmd, "@K_CH_ANIO_TRASLADO", DbType.String, oItem.AnioTrasladoCh);
                    db.AddInParameter(cmd, "@K_NU_ID_TRASLADO", DbType.Decimal, oItem.IdTrasladoNu);
                    map = 6;

                }
                else
                {
                    db.AddInParameter(cmd, "@NU_ID_EMPRESA", DbType.Decimal, oItem.IdEmpresaNu);
                    db.AddInParameter(cmd, "@NU_ID_SUCURSAL", DbType.Decimal, oItem.IdSucursalNu);
                    db.AddInParameter(cmd, "@NU_ID_ALMACEN_ORIG", DbType.Decimal, oItem.IdAlmacenOrigNu);
                    db.AddInParameter(cmd, "@NU_ID_SUCURSAL_DEST", DbType.Decimal, oItem.IdSucursalDestNu);
