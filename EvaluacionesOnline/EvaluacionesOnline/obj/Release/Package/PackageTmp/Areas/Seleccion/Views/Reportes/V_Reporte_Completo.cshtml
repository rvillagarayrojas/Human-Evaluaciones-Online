@using EvaluacionesOnline.Areas.Sistemas.Models
@{

    ViewBag.Title = "SISTEMA DE EVALUACIONES PSICOLABORALES - TEST SMART";
    Layout = "~/Areas/Seleccion/Views/MPEvaluacionesPrint.cshtml";
    var Sesion = (SesionModel)Session[SesionModel.SessionName];
}
<link href="~/Content/Print.css" rel="stylesheet" type="text/css" media="print"/>
<link href="~/Content/Doc.css" rel="stylesheet" type="text/css"/>
<style type="text/css">
    #container, #sliders {
	    min-width: 310px; 
	    max-width: 800px;
	    margin: 0 auto;
    }
    #container {
	    height: 400px; 
    }
</style>

<script type="text/javascript">

    var Esperado1, Obtenido1, Esperado2, Obtenido2, Esperado3, Obtenido3, Valor_D, Valor_I, Valor_S, Valor_C;

    var CategoriasCPS   = []
    var ValoresCPS      = [];
    var ValoresCPSS     = [];

    var PerfilesKostick = []
    var PuntajesKostick = [];

</script>


@if (Model.ls_wonderlik != null && Model.ls_wonderlik.Count > 0)
{
    <script type="text/javascript">
        Esperado1 = "@Model.ls_wonderlik[0].me_prueba.reporte_conocimiento.nu_rango_menor_esperado";
        Obtenido1 = "@Model.ls_wonderlik[0].me_prueba.reporte_conocimiento.nu_parametro_obtenido";
    </script>
}

@if (Model.ls_gatb != null && Model.ls_gatb.Count > 0)
{
    <script type="text/javascript">
        Esperado2   = "@Model.ls_gatb[0].me_prueba.reporte_conocimiento.nu_rango_menor_esperado";
        Obtenido2   = "@Model.ls_gatb[0].me_prueba.reporte_conocimiento.nu_parametro_obtenido";
        Obtenido2_v = "@Model.ls_gatb[0].me_prueba.reporte_conocimiento.nu_parametro_obtenido_v";
        Obtenido2_n = "@Model.ls_gatb[0].me_prueba.reporte_conocimiento.nu_parametro_obtenido_n";
    </script>
}

@if (Model.ls_ic != null && Model.ls_ic.Count > 0)
{
    <script type="text/javascript">
        Esperado3 = parseInt("@Model.ls_ic[0].me_prueba.reporte_conocimiento.nu_categoria_esperada");
        Obtenido3 = parseInt("@Model.ls_ic[0].me_prueba.reporte_conocimiento.nu_categoria_obtenida");
    </script>
}

@if (Model.ls_disc != null && Model.ls_disc.Count > 0)
{
    <script type="text/javascript">
        Valor_D = parseInt("@Model.ls_disc[0].me_prueba.reporte_conocimiento.vc_patron_disc[0]");
        Valor_I = parseInt("@Model.ls_disc[0].me_prueba.reporte_conocimiento.vc_patron_disc[1]");
        Valor_S = parseInt("@Model.ls_disc[0].me_prueba.reporte_conocimiento.vc_patron_disc[2]");
        Valor_C = parseInt("@Model.ls_disc[0].me_prueba.reporte_conocimiento.vc_patron_disc[3]");
    </script>
}

<script type="text/javascript">

    CategoriasCPS[CategoriasCPS.length] = "<strong>PERCENTIL MÁXIMO</strong>";
    ValoresCPSS[ValoresCPSS.length]     = 99;

</script>

@if (Model.ls_cps != null && Model.ls_cps.Count > 0)
{
    foreach(var item in Model.ls_cps)
    {
        <script type="text/javascript">

            CategoriasCPS[CategoriasCPS.length] = "@item.me_prueba.reporte_conocimiento.vc_parametro_cps";
            ValoresCPS[ValoresCPS.length]       = parseInt("@item.me_prueba.reporte_conocimiento.nu_marcadas");
            ValoresCPSS[ValoresCPSS.length]     = parseInt("@item.me_prueba.reporte_conocimiento.nu_valor_s");
        </script>
    }
}

@if (Model.ls_kostick != null && Model.ls_kostick.Count > 0)
{
    foreach (var item in Model.ls_kostick)
    {
        <script type="text/javascript">
            PerfilesKostick[PerfilesKostick.length] = "@item.me_prueba.reporte_conocimiento.vc_desc_perfil";
            PuntajesKostick[PuntajesKostick.length] = parseInt("@item.me_prueba.reporte_conocimiento.nu_marcadas");
        </script>
    }
}


<script src="~/Scripts/Sistemas/jquerydb.js"></script>
<script type="text/javascript">

    var Categorias1     = [], Calificacion1     = [], Calificacion11    = [];
    var Categorias2     = [], Calificacion2E    = [];
    var Calificacion2O  = [];
    var Categorias3     = [], Calificacion3     = [], Calificacion33 = [];

    $(document).ready(function () {

        Categorias1[Categorias1.length] = "RESULTADO";
        Calificacion1[Calificacion1.length] = parseInt(Esperado1);
        Calificacion11[Calificacion11.length] = parseInt(Obtenido1);

        Categorias2[Categorias2.length] = "G";
        Categorias2[Categorias2.length] = "V";
        Categorias2[Categorias2.length] = "N";
        Calificacion2E[Calificacion2E.length] = parseInt(Esperado2);
        Calificacion2O[Calificacion2O.length] = parseInt(Obtenido2);
        Calificacion2E[Calificacion2E.length] = parseInt(Esperado2);
        Calificacion2O[Calificacion2O.length] = parseInt(Obtenido2_v);
        Calificacion2E[Calificacion2E.length] = parseInt(Esperado2);
        Calificacion2O[Calificacion2O.length] = parseInt(Obtenido2_n);


        Categorias3[Categorias3.length]         = "RESULTADO";
        Calificacion3[Calificacion3.length]     = parseInt(Esperado3);
        Calificacion33[Calificacion33.length]   = parseInt(Obtenido3);

        Load1();

    });

    function Load1() {
        $('#container1').html('');
        $('#container2').html('');
        $('#container3').html('');

        var chart1 = new Highcharts.Chart({
            chart: {
                renderTo: 'container1',
                type: 'bar',
                marginTop: 80,
                marginRight: 40
            },

            title: {
                text: '<strong>RESULTADO DEL TEST DE WONDERLIC</strong>'
            },

            xAxis: {
                categories: Categorias1
            },
            yAxis: {
                allowDecimals: false,
                min: 0,
                title: {
                    text: 'ESCALA DE CANTIDAD'
                },
                stackLabels: {
                    enabled: true,
                    style: {
                        fontWeight: 'bold',
                        fontsize: '18px',
                        color: (Highcharts.theme && Highcharts.theme.textColor) || 'darkred'
                    }
                }
            },
            credits: {
                enabled: false
            },

            series: [{
                name: 'ESPERADO',
                data: Calificacion1
            },{
                name: 'OBTENIDO',
                data: Calificacion11
            }]
        });
    

        var chart2 = new Highcharts.Chart({
            chart: {
                renderTo: 'container2',
                type: 'column',
                marginTop: 80,
                marginRight: 40
            },

            title: {
                text: '<strong>RESULTADO GATB</strong>'
            },

            xAxis: {
                categories: Categorias2
            },
            yAxis: {
                allowDecimals: false,
                min: 0,
                title: {
                    text: 'ESCALA DE CANTIDAD'
                },
                stackLabels: {
                    enabled: true,
                    style: {
                        fontWeight: 'bold',
                        fontsize: '18px',
                        color: (Highcharts.theme && Highcharts.theme.textColor) || 'darkred'
                    }
                }
            },
            credits: {
                enabled: false
            },
            series: [{
                name: 'ESPERADO',
                data: Calificacion2E
            }, {
                name: 'OBTENIDO',
                data: Calificacion2O
            }]
        });




        var chart3 = new Highcharts.Chart({
            chart: {
                renderTo: 'container3',
                type: 'bar',
                marginTop: 80,
                marginRight: 40
            },

            title: {
                text: '<strong>RESULTADO IC</strong>'
            },

            xAxis: {
                categories: Categorias3
            },
            yAxis: {
                allowDecimals: false,
                min: 0,
                title: {
                    text: 'ESCALA DE CATEGORÍAS'
                },
                stackLabels: {
                    enabled: true,
                    style: {
                        fontWeight: 'bold',
                        fontsize: '18px',
                        color: (Highcharts.theme && Highcharts.theme.textColor) || 'darkred'
                    }
                }
            },
            credits: {
                enabled: false
            },

            series: [{
                name: 'ESPERADO',
                data: Calificacion3
            }, {
                name: 'OBTENIDO',
                data: Calificacion33
            }]
        });


        var chart4 = new Highcharts.Chart({
            chart: {
                renderTo: 'container4',
                type: 'bar'
            },
            title: {
                text: '<strong>RESULTADO DISC</strong>'
            },
            xAxis: {
                categories: ['RESULTADO']
            },
            yAxis: {
                allowDecimals: false,
                min: 0,
                title: {
                    text: 'ESCALA DE VALOR'
                },
                stackLabels: {
                    enabled: true,
                    style: {
                        fontWeight: 'bold',
                        fontsize: '18px',
                        color: (Highcharts.theme && Highcharts.theme.textColor) || 'darkred'
                    }
                }
            },

            tooltip: {
                headerFormat: '<b>{point.key}</b><br>',
                pointFormat: '<span style="color:{series.color}">\u25CF</span> {series.name}: {point.y} / {point.stackTotal}'
            },
            tooltip: {
                formatter: function () {
                    return '<b>' + this.x + '</b><br/>' +
                        this.series.name + ': ' + this.y + '<br/>';
                }
            },

            credits: {
                enabled: false
            },
            series: [{
                name: 'D',
                data: [Valor_D]
            }, {
                name: 'I',
                data: [Valor_I]
            }, {
                name: 'S',
                data: [Valor_S]
            }, {
                name: 'C',
                data: [Valor_C]
            }]
        });


        var chart5 = new Highcharts.Chart({
            chart: {
                renderTo: 'container5',
                type: 'bar',
                spacingBottom: 30,
                height: 750
            },
            title: {
                text: '<strong>RESULTADO CPS</strong>'
            },
            subtitle: {
                floating: true,
                align: 'right',
                verticalAlign: 'bottom',
                y: 15
            },
            xAxis: {
                categories: CategoriasCPS,
                labels: {
                    style: {
                        fontSize: '13px',
                    }
                }
            },
            yAxis: {
                title: {
                    text: 'Percentil Obtenido'
                },
                labels: {
                    formatter: function () {
                        return this.value;
                    },
                    style: {
                        fontSize: '14px',
                    }
                },
                scalable: false,
                max: 100
            },
            tooltip: {
                formatter: function () {
                    return '<b>' + this.series.name + '</b><br/>' +
                        this.x + ': ' + this.y;
                }
            },
            plotOptions: {
                area: {
                    fillOpacity: 0.5
                }
            },
            credits: {
                enabled: false
            },
            series: [{
                name: 'Percentil Obtenido',
                data: ValoresCPSS
            }]
        });



        var chart6 = new Highcharts.Chart({
            chart: {
                renderTo: 'container6',
                polar: true,
                type: 'line'
            },

            title: {
                text: '<strong>PERFILES DE COMPETENCIA LABORAL - KOSTICK</strong>',
                x: -80
            },

            pane: {
                size: '90%'
            },

            xAxis: {
                categories: PerfilesKostick,
                tickmarkPlacement: 'on',
                lineWidth: 0
            },

            yAxis: {
                gridLineInterpolation: 'polygon',
                lineWidth: 0,
                min: 0
            },

            tooltip: {
                shared: true,
                pointFormat: '<span style="color:{series.color}">{series.name}: <b>${point.y:,.0f}</b><br/>'
            },

            legend: {
                align: 'right',
                verticalAlign: 'top',
                y: 70,
                layout: 'vertical'
            },

            series: [{
                name: 'PUNTAJES',
                data: PuntajesKostick,
                pointPlacement: 'on'
            }]

        });
        
    }

</script>
<script src="~/Recursos/Db/js/highcharts.js"></script>
