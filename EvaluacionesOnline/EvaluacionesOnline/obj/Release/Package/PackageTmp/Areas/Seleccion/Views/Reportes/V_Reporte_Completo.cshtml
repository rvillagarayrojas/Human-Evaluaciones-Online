<script type="text/javascript">

    $(document).ready(function () {
        $("#btnImprimir").click(function () {
            $('#Contenido').printArea();
        });
        BuscarAttr();
        var f = new Date();
        $('#Fecha').html(("0" + f.getDate()).slice(-2) + "/" + ("0" + (f.getMonth() + 1)).slice(-2) + "/" + f.getFullYear());
        $('#DescGatb').html($('#DescGatb').text());
        RemoveCps();
    });

    function BuscarAttr() {

        cronometro = setInterval
                     (
                        function () {
                            
                            $('[fill]').each(function () {

                                if ($(this).attr('fill') == '#7cb5ec') {
                                    $(this).attr('fill', 'rgb(244,209,170)');
                                }
                                if ($(this).attr('fill') == '#434348') {
                                    $(this).attr('fill', 'rgb(235,110,25)');
                                }
                                if ($(this).attr('fill') == '#f7a35c') {
                                    $(this).attr('fill', 'rgba(255, 241, 77, 0.82)');
                                }

                            });

                        }, 10
                     );
    }

    function RemoveCps() {
        var cont = 0;
        $('#container5 .highcharts-grid path').each(function () {
            if (cont > 10) { $(this).remove(); }
            cont++;
        }); 
        cont = 0;
        $('#container5 .highcharts-yaxis-labels text').each(function () {
            if (cont > 10) { $(this).remove(); }
            cont++;
        });
    }

</script>
