                             ConocimientoDetalle: ConocimientoDetalle,
                             ExperienciaDetalle: ExperienciaDetalle,
                             FamiliaresDetalle: FamiliaresDetalle
                        }
        var mme_prueba = { me_prueba: me_prueba }

        model.mme_prueba = mme_prueba;
        //return false;

        $.ajax({
            url: "@Url.RouteUrl("AC_ActualizarDatosCandidatoFicha")",
            type: "POST",
            data: JSON.stringify(model),
            contentType: "application/json; charset=utf-8",
            success: function (result) {

                if (result.trans == 1) {
                    $('#ContenedorGif').html('<i class="glyphicon glyphicon-ok" style="color:green"></i> Sus datos se han actualizado correctamente; ahora lo llevaremos a realizar su evaluación.<br/><div class="col-md-12 text-center"><img src="/Recursos/Gif/Cargando.gif" style="max-width:80px;"/></div><br/><label>Cargando pruebas...</label>');

                    var interRedirect = 0;
                    redirec = setInterval
                     (
                        function () {
                            interRedirect++;
                            if (interRedirect == 4) {
                                clearInterval(redirec); window.location.href = "@Url.RouteUrl("V_Pruebas")";
                            }
                        }, 1000
                     );
                    }
            },
            error: function (result) {
                $('#ModalDeMensaje').hide();
                $('#ModalNotificacion_Contenido').html("Ha ocurrido un error inesperado no se ha podido actualizar sus datos, de persistir el inconveniente contactar con el área de sistemas.")
                $('#ModalNotificacion').show();
            }
        });
      }
</script>